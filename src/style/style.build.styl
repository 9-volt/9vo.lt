// System imports
@import "nib"

@import "normalize"

$white = #fefef4
$green = #2fbeb6
$red = #ef4a48
$redDarken = #e24849

@import "utils"

body {
  font-family: 'Voces', cursive;
  font-size-adjust:0.5;
  font-weight:200;
  font-style:normal;
  background $white
}

h1, h2, h3, h4, h5, h6 {
  font-family:"Monosten A", Courier, monospace;
  font-size-adjust:0.5;
  font-weight:bold;
  font-style:normal;
}

@import "responsive-768px-991px" // implicit style
@import "responsive-1200px-max"
@import "responsive-992px-1199px"
@import "responsive-min-767px"
@import "responsive-min-480px"

